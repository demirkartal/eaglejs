{
  "source": {
    "include": ["README.md", "dist/eaglejs.esm.js"]
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "includeDate": false,
      "outputSourceFiles": false
    }
  },
  "opts": {
    "template": "node_modules/docdash/",
    "encoding": "utf8",
    "destination": "docs/"
  },
  "docdash": {
    "sort": true,
    "search": true,
    "collapse": true,
    "sectionOrder": ["Modules", "Classes"],
    "meta": {
      "title": "EagleJS",
      "description": "EagleJS is a jQuery-Like DOM manipulation library for modern browsers."
    },
    "menu": {
      "Github Repo": {
        "href": "https://github.com/demirkartal/eaglejs",
        "target": "_blank",
        "class": "menu-item",
        "id": "website_link"
      }
    }
  }
}
